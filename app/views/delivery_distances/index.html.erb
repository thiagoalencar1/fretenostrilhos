<h1 class="text-center">Intervalos de Distância</h1>
<h2 class="text-center">para cadastro de prazos</h2>
<br />
<table class="table text-center align-middle m-auto" style="max-width: 900px">
  <thead>
    <tr>
      <th class="text-right">#</th>
      <th>De</th>
      <th>Até</th>
      <th>Ação</th>
    </th>
  </thead>
  <tbody>
    <% @delivery_distances.each do |dd| %>
    <tr>
      <td><%= dd.id %></td>
      <td><%= dd.from_km %>km</td>
      <td><%= dd.to_km %>km</td>
      <td><%= link_to 'Editar', edit_delivery_distance_path(dd), class: 'btn btn-outline-warning' %></td>
    <% end %>
  </tbody>
</table>